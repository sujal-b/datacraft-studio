{
  "name": "project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:backend": "cd backend && python -m uvicorn main:app --reload",
    "start:celery": "cd backend && python -m celery -A celery_worker.celery_app worker --loglevel=info --pool=solo",
    "start:all": "concurrently \"npm:dev\" \"npm:start:backend\" \"npm:start:celery\"",
    "build": "chmod +x ./node_modules/.bin/vite && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "ag-grid-react": "^34.0.2",
    "papaparse": "^5.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
